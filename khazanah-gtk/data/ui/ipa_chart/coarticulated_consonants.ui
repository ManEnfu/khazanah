<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.6"/>
  <requires lib="Adw" version="1.2"/>

  <!-- IPA chart of co-articulated consonants. -->
  <template class="KhzIpaCoarticulatedConsonantsChart" parent="AdwBin">
    <child>
      <object class="GtkGrid" id="chart"> 
        <property name="halign">center</property>
        <style>
          <class name="ipa-chart"/>
          <class name="card"/>
        </style>
        <property name="row-spacing">1</property>
      </object>
    </child>
  </template> <!-- KhzIpaCoarticulatedConsonantsChartt --> 

</interface>
